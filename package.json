{
  "name": "three-ts-template",
  "version": "1.0.0",
  "description": "Personal template project for three.js + typescript development.",
  "main": "index.js",
  "author": "Andrés Valencia Téllez",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier -l \"**/*.ts\" \"**/*.tsx\" \"**/*.json\"",
    "prettier:fix": "prettier --write \"**/*.ts\" \"**/*.tsx\" \"**/*.json\"",
    "test": "yarn prettier",
    "fix": "yarn prettier:fix",
    "build": "webpack --config webpack/prod.config.js",
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "start:local": "yarn start --host 0.0.0.0"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "copy-webpack-plugin": "^5.1.1",
    "guify": "^0.12.0",
    "html-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "stats.js": "^0.17.0",
    "three": "^0.123.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@types/stats.js": "^0.17.0",
    "glslify-loader": "^2.0.0",
    "raw-loader": "^4.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
